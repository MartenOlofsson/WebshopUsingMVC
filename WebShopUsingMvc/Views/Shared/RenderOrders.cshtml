@using WebShop.DataCore.Models
@model List<Order>
@{
    ViewBag.Title = "Orders";
}
@if (Model.Count > 0)
{ 
    
    <div id="orders">
        <table>
            <tr>
                <td>
                    <span>Mina ordrar</span>
                </td>
                @foreach (var orders in Model)
                {
                    <tr>
                    <td>
                    @orders.OrderTime OrderId: @orders.Id
                    </td>
                    </tr>
                }
            </tr>
        </table>
    </div>

}
