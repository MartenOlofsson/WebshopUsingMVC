@using WebShop.DataCore.Models
@model List<Search>
@{
    ViewBag.Title = "Search";
}
@if (Model.Count > 0)
{ 
    <div id="searches">
        <table>
            <tr>
                <td>
                    <span>
                        Senaste sökningar</span>
                </td>
                @foreach (var search in Model)
                {
                    <tr>
                    <td>
                    <a href="http://localhost:64295/Home/Search?search=@search.SearchString">@search.SearchString
                    </a>
                    </td>
                    </tr>
                }
            </tr>
        </table>
    </div>
}
